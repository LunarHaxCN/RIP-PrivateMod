{
    "type": "Menu",
    "title": "InfiniteAuraRIP",
    "title_bg_color": "#B6D6F2",
    "title_start_color": "#00BFFF",
    "title_end_color": "#8A2BE2",
    "color": "#FFFFFF",
    "alpha": 0.9,
    "can_close": true,
    "hide": true,
    "items": [
        {
            "type": "TextView",
            "name": "项目地址\ngithub.com/MoYuanCN/RIP-PrivateMod/",
            "color": "#3D3D3D",
            "size": 12,
            "padding": [5, 5, 5, 5]
        },
        {
            "type": "TextView",
            "name": "UI原作者：MoYuanCN&LunarHax",
            "color": "#3D3D3D",
            "size": 12,
            "padding": [5, 5, 5, 5]
        },
        {
            "type": "TextView",
            "name": "加载InfiniteAura",
            "color": "#3D3D3D",
            "size": 12,
            "padding": [5, 5, 5, 5],
            "load_script": "InfiniteAura - command.js"
        },
        {
            "type": "TextView",
            "name": "打开配置界面\n请确保已加载Js",
            "color": "#3D3D3D",
            "size": 12,
            "padding": [5, 5, 5, 5],
            "send_message": "set"
        },
        {
            "type": "TextView",
            "name": "开关InfiniteAura",
            "color": "#3D3D3D",
            "size": 12,
            "padding": [5, 5, 5, 5],
            "run_command": "/InfiniteAura Attack",
            "shortcut": {
                "type": "ClickButton",
                "params": [65, 65],
                "no_circle": true,
                "run_command": "/InfiniteAura Attack",
                "icon": "InfiniteAuraRIP.png"
            }
        },
        {
            "type": "TextView",
            "name": "开关自动模式",
            "color": "#3D3D3D",
            "size": 12,
            "padding": [5, 5, 5, 5],
            "run_command": "/InfiniteAura AutoMode"
        },
        {
            "type": "Switch",
            "name": "智能队友依赖项\n请开启智能队友前打开",
            "color": "#7B68EE",
            "size": 10,
            "padding": [5, 5, 5, 5],
            "tag": "fun_smart_team"
        },
        {
            "type": "TextView",
            "name": "开关智能队友",
            "color": "#3D3D3D",
            "size": 12,
            "padding": [5, 5, 5, 5],
            "run_command": "/InfiniteAura Team"
        },
        {
            "type": "TextView",
            "name": "模式切换",
            "color": "#3D3D3D",
            "size": 12,
            "padding": [5, 5, 5, 5],
            "run_command": "/InfiniteAura MOVEMode"
        },
        {
            "type": "TextView",
            "name": "Disabler提示",
            "color": "#3D3D3D",
            "size": 12,
            "padding": [5, 5, 5, 5],
            "load_script": "DisablerTip.js"
        },
        {
            "type": "Switch",
            "name": "Disabler",
            "color": "#7B68EE",
            "size": 10,
            "padding": [5, 5, 5, 5],
            "tag": "fun_multiple_packet",
            "shortcut": {
                "type": "CheckedButton",
                "params": [65, 65],
                "no_circle": true,
                "off": "InfiniteAuraRIP_DisOff.png",
                "on": "InfiniteAuraRIP_DisOn.png",
                "icon": "InfiniteAuraRIP_DisOn.png"
            },
            "items": [
                {
                    "type": "SeekBar",
                    "key": "count",
                    "format": "数量 %d",
                    "color": "#63bbd0",
                    "size": 8,
                    "padding": [0, 0, 0, 0],
                    "value": 5,
                    "min": 1,
                    "max": 10
                }
            ]
        },
        {
            "type": "TextView",
            "name": "设置当前高度为\n最高高度",
            "color": "#3D3D3D",
            "size": 12,
            "padding": [5, 5, 5, 5],
            "run_command": "/InfiniteAura Set_Y_MAX"
        },
        {
            "type": "TextView",
            "name": "退出脚本",
            "color": "#3D3D3D",
            "size": 12,
            "padding": [5, 5, 5, 5],
            "run_command": "/InfiniteAura exit"
        }
    ]
}
